  def binomialCoefficient(n: Int, k: Int) ={
    if (k > n/2.0)
    {
    val k2=n-k
    (BigInt(n - k2 + 1) to n).product /
    (BigInt(1) to k2).product
    }
    else
    {
    (BigInt(n - k + 1) to n).product /
    (BigInt(1) to k).product
    }
    }                                             //> binomialCoefficient: (n: Int, k: Int)scala.math.BigInt
    
